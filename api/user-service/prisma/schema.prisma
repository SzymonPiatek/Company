generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("USER_DB_URL")
}

model User {
  id        String  @id @default(uuid())
  email     String  @unique
  firstName String
  lastName  String
  password  String
  isActive  Boolean @default(false)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
